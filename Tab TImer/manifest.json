{
    "name": "Tab Timer",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Track how long each tab is active.",
    "permissions": [
      "tabs",
      "storage",
      "alarms"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  